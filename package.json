{
  "name": "gatsby-starter-ghost",
  "description": "A starter template to build lightning fast websites with Ghost and Gatsby",
  "version": "1.0.0",
  "license": "MIT",
  "author": "Ghost Foundation",
  "homepage": "https://docs.ghost.org/api/gatsby/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tryghost/gatsby-starter-ghost.git"
  },
  "engines": {
    "node": ">= 10.13.0"
  },
  "bugs": {
    "url": "https://github.com/tryghost/gatsby-starter-ghost/issues"
  },
  "keywords": [
    "gatsby",
    "ghost"
  ],
  "main": "n/a",
  "scripts": {
    "serve": "gatsby build && NODE_ENV=production gatsby serve",
    "postinstall": "cd functions/preview && yarn",
    "build": "NODE_ENV=production gatsby build",
    "clean": "gatsby clean",
    "start": "GATSBY_GRAPHQL_IDE=playground npx gatsby develop",
    "lint": "eslint . --ext .js --cache",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,md,scss}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "babel-eslint": "10.1.0",
    "eslint": "7.17.0",
    "eslint-config-babel": "9.0.0",
    "eslint-config-prettier": "7.1.0",
    "eslint-plugin-ghost": "2.0.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-prettier": "3.3.1",
    "husky": "4.3.6",
    "lint-staged": "10.5.3",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-standard": "5.0.0",
    "prettier": "2.2.1"
  },
  "dependencies": {
    "@graphcomment/graphcomment": "1.0.10",
    "@tryghost/helpers": "1.1.35",
    "@tryghost/helpers-gatsby": "1.0.40",
    "cheerio": "1.0.0-rc.5",
    "classnames": "2.2.6",
    "fitvids": "2.1.1",
    "gatsby": "2.29.3",
    "gatsby-awesome-pagination": "0.3.6",
    "gatsby-image": "2.8.0",
    "gatsby-plugin-advanced-sitemap": "1.5.6",
    "gatsby-plugin-catch-links": "2.7.0",
    "gatsby-plugin-create-client-paths": "2.7.0",
    "gatsby-plugin-feed": "2.10.0",
    "gatsby-plugin-force-trailing-slashes": "1.0.4",
    "gatsby-plugin-load-script": "1.1.0",
    "gatsby-plugin-manifest": "2.9.1",
    "gatsby-plugin-offline": "3.7.1",
    "gatsby-plugin-react-helmet": "3.7.0",
    "gatsby-plugin-remote-images": "2.2.0",
    "gatsby-plugin-sass": "2.8.0",
    "gatsby-plugin-sharp": "2.11.2",
    "gatsby-rehype-inline-images": "1.9.3",
    "gatsby-rehype-prismjs": "1.2.5",
    "gatsby-source-filesystem": "2.8.1",
    "gatsby-source-ghost": "4.2.2",
    "gatsby-transformer-rehype": "1.9.8",
    "gatsby-transformer-sharp": "2.9.0",
    "lodash": "4.17.20",
    "node-sass": "4.14.1",
    "normalize.css": "8.0.1",
    "prismjs": "1.23.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-helmet": "6.1.0",
    "react-instagram-embed": "2.0.0",
    "react-player": "2.7.2",
    "react-string-replace": "0.4.4",
    "rehype-react": "6.1.0",
    "rough-notation": "0.5.1",
    "scroll-precent": "1.0.0",
    "smooth-smooth-scroll": "1.5.2"
  }
}
